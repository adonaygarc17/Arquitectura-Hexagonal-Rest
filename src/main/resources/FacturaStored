CREATE OR REPLACE PROCEDURE createFactura(f_id IN NUMBER, f_documento IN VARCHAR2,
f_facturaTax IN NUMBER, f_facturaSubTotal IN NUMBER, f_totalDiscount IN NUMBER, f_total IN NUMBER)
AS BEGIN
INSERT INTO factura(id,numberoDocumento, facturaTax,facturaSubTotal,totalDiscount,total)
VALUES (f_id , f_documento,f_facturaTax, f_facturaSubTotal , f_totalDiscount, f_total)
COMMIT
END;

CREATE OR REPLACE PROCEDURE createProduct(p_idProduct IN NUMBER,p_id IN NUMBER, p_title IN VARCHAR2,
p_categoria IN VARCHAR2, p_marca IN VARCHAR2, p_thumbnail IN VARCHAR2, p_porcentajeDescuento IN NUMBER,p_montoDescuento IN NUMBER, p_precioFinal IN NUMBER)
AS BEGIN
INSERT INTO factura(id,title,categoria,marca,thumbnail,porcentajeDescuento, montoDescuento,precioFinal)
VALUES (p_idProduct, p_id, p_title,p_categoria, p_marca, p_thumbnail, p_porcentajeDescuento,p_montoDescuento, p_precioFinal)
COMMIT
END;